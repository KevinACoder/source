data_struct_demo: main.o double_linked_list.o
	g++ -o data_struct_demo main.o double_linked_list.o

main.o: main.cpp struct_defines.h
	g++ -c main.cpp

double_linked_list.o: double_linked_list.cpp struct_defines.h
	g++ -c double_linked_list.cpp

